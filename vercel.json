{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/helpers.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/database_setup.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/helpers",
      "dest": "api/helpers.py"
    },
    {
      "src": "/database_setup",
      "dest": "api/database_setup.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ]
}
